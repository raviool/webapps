<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="t124003.backend.model.Employee" table="employee" schema="public" catalog="t124003_dokumendid">
        <id name="employee">
            <column name="employee" sql-type="numeric" precision="10" not-null="true"/>
        </id>
        <property name="personFk">
            <column name="person_fk" sql-type="numeric" precision="10"/>
        </property>
        <property name="enterpriseFk">
            <column name="enterprise_fk" sql-type="numeric" precision="10"/>
        </property>
        <property name="structUnitFk">
            <column name="struct_unit_fk" sql-type="numeric" precision="10"/>
        </property>
        <property name="active">
            <column name="active" sql-type="varchar" length="1"/>
        </property>
    </class>
</hibernate-mapping>