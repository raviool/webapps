<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="t124003.backend.model.EmployeeRole" table="employee_role" schema="public"
           catalog="t124003_dokumendid">
        <id name="employeeRole">
            <column name="employee_role" sql-type="numeric" precision="10" not-null="true"/>
        </id>
        <property name="employeeFk">
            <column name="employee_fk" sql-type="numeric" precision="10"/>
        </property>
        <property name="employeeRoleTypeFk">
            <column name="employee_role_type_fk" sql-type="numeric" precision="10"/>
        </property>
        <property name="active">
            <column name="active" sql-type="varchar" length="1"/>
        </property>
    </class>
</hibernate-mapping>