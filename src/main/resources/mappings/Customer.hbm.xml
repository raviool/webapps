<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="t124003.backend.model.Customer" table="customer" schema="public" catalog="t124003_dokumendid">
        <id name="customer">
            <column name="customer" sql-type="numeric" precision="10" not-null="true"/>
        </id>
        <property name="subjectFk">
            <column name="subject_fk" sql-type="numeric" precision="10"/>
        </property>
        <property name="subjectTypeFk">
            <column name="subject_type_fk" sql-type="numeric" precision="10"/>
        </property>
    </class>
</hibernate-mapping>